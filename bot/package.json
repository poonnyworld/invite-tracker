{
  "name": "invite-tracker",
  "version": "1.0.0",
  "description": "Discord bot for tracking invite links and server joins",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "watch": "tsc --watch",
    "deploy": "ts-node src/deploy-commands.ts",
    "deploy:prod": "node dist/deploy-commands.js"
  },
  "keywords": [
    "discord",
    "bot",
    "invite",
    "tracking"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "mongoose": "^8.0.3",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
